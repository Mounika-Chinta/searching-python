def linear_search(arr,target):
    for i in range (len(arr)):
        if arr[i]==target:
            return i
            return -1
arr=[10,20,30,40,50]
target=int(input('enter your target value'))
result=linear_search(arr,target)
if result!=-1:
  print("found",result)
else:
  print("element not found")
